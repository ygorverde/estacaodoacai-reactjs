te({ list: resp.data })\r\n        })\r\n    }\r\n\r\n    clear() {\r\n        this.setState({ product: initialState.product })\r\n    }\r\n\r\n    save() {\r\n        const product = this.state.product\r\n        const method = product.id ? 'put' : 'post'\r\n        const url = product.id ? `${baseUrl}/${product.id}` : baseUrl\r\n        axios[method](url, product).then(resp => {\r\n            const list = this.getUpdatedList(resp.data)\r\n            this.setState({ product: initialState.product, list })\r\n        })\r\n    }\r\n\r\n    getUpdatedList(product, add = true) {\r\n        const list = this.state.list.filter(p => p.id !== product.id)\r\n        if (add) list.unshift(product)\r\n        return list\r\n    }\r\n\r\n    updateField(event) {\r\n        const product = { ... this.state.product }\r\n        product[event.target.name] = event.target.value\r\n        this.setState({ product })\r\n    }\r\n\r\n    renderForm() {\r\n        return (\r\n            <div className=\"Form\">\r\n                <div className=\"row\">\r\n                    <div className=\"col-12 col-md-4\">\r\n                        <div className=\"form-group\">\r\n                            <label>Nome do produto</label>\r\n                            <input type=\"text\" className=\"form-control\"\r\n                                name=\"productName\"\r\n                                value={this.state.product.productName}\r\n                                onChange={e => this.updateField(e)}\r\n                                placeholder=\"Digite o nome do produto\"\r\n                            />\r\n                        </div>\r\n                    </div>\r\n                    <div className=\"col-12 col-md-6\">\r\n                        <div className=\"form-group\">\r\n                            <label>Descrição do produto</label>\r\n                            <input type=\"text\" className=\"form-control\"\r\n                                name=\"description\"\r\n                                value={this.state.product.description}\r\n                                onChange={e => this.updateField(e)}\r\n                                placeholder=\"Ex: Banana, gotas e jujuba\"\r\n                            />\r\n                        </div>\r\n                    </div>\r\n                    <div className=\"col-12 col-md-2\">\r\n                        <div className=\"form-group\">\r\n                            <label>Tamanho</label>\r\n                            <select name=\"size\" className=\"form-control\" value={this.state.product.size} onChange={e => this.updateField(e)}>\r\n                                <option value=\"\">Selecione</option>\r\n                                <option value=\"300\">300ml</option>\r\n                                <option value=\"500\">500ml</option>\r\n                                <option value=\"1000\">1000ml</option>\r\n                            </select>\r\n                        </div>\r\n                    </div>\r\n                    <div className=\"col-12 col-md-2\">\r\n                        <div className=\"form-group\">\r\n                            <label>Sabor</label>\r\n                            <select name=\"flavor\" className=\"form-control\" value={this.state.product.flavor} onChange={e => this.updateField(e)}>\r\n                                <option value=\"\">Selecione</option>\r\n                                <option value=\"Morango\">Morango</option>\r\n                                <option value=\"Tradicional\">Tradicional</option>\r\n                                <option value=\"Ambos\">Ambos</option>\r\n                            </select>\r\n                        </div>\r\n                    </div>\r\n                    <div className=\"col-12 col-md-2\">\r\n                        <div className=\"form-group\">\r\n                            <label>Valor</label>\r\n                            <input type=\"text\" className=\"form-control\"\r\n                                name=\"price\"\r\n                                value={this.state.product.price}\r\n                                onChange={e => this.updateField(e)}\r\n                                placeholder=\"Ex: 9.99\"\r\n                            />\r\n                        </div>\r\n                    </div>\r\n                    <div className=\"col-12 d-flex justify-content-end col-md-8 mt-4 mb-4\">\r\n                        <button className=\"btn btn-primary\" onClick={e => this.save(e)}>\r\n                            Salvar\r\n                        </button>\r\n\r\n                        <button className=\"btn btn-secondary ml-2\" onClick={e => this.clear(e)}>\r\n                            Cancelar\r\n                        </button>\r\n                    </div>\r\n\r\n                    <div className=\"col-12 col-md-2\">\r\n                        <div className=\"form-group\">\r\n                            <label>Produtos</label>\r\n                            <select name=\"selectproduct\" className=\"form-control\" value={this.state.product.id} onChange={e => this.loadFields(e)}>\r\n                                <option value=\"\">Selecione</option>\r\n                                {this.renderOptions()}\r\n                            </select>\r\n                        </div>\r\n                    </div>\r\n\r\n                </div>\r\n            </div>\r\n        )\r\n    }\r\n\r\n    load(product) {\r\n        this.setState({ product })\r\n    }\r\n\r\n    loadFields(e) {\r\n        const fieldid = e.target.value\r\n        axios.get(`${baseUrl}/${fieldid}`).then(resp => {\r\n            const product = resp.data\r\n            this.setState({ product: product })\r\n        })\r\n    }\r\n\r\n    renderOptions() {\r\n        return this.state.list.map(product => {\r\n            return (\r\n                <option value={product.id} key={product.id}>{product.productName}</option>\r\n            )\r\n        })\r\n    }\r\n\r\n    remove(product) {\r\n        axios.delete(`${baseUrl}/${product.id}`).then(resp => {\r\n            const list = this.getUpdatedList(product, false) // Procurando na lista\r\n            this.setState({ list })\r\n        })\r\n    }\r\n\r\n    renderTable() {\r\n        return (\r\n            <table className=\"table mt-0\">\r\n                <thead>\r\n                    <tr>\r\n                        <th>ID</th>\r\n                        <th>Nome</th>\r\n                        <th>Descrição</th>\r\n                        <th>Tamanho</th>\r\n                        <th>Sabor</th>\r\n                        <th>Valor</th>\r\n                    </tr>\r\n                </thead>\r\n                <tbody>\r\n                    {this.renderRows()}\r\n                </tbody>\r\n\r\n            </table>\r\n        )\r\n    }\r\n\r\n    renderRows() {\r\n        return this.state.list.map(product => {\r\n            return (\r\n                <tr key={product.id}>\r\n                    <td>{product.id}</td>\r\n                    <td>{product.productName}</td>\r\n                    <td>{product.description}</td>\r\n                    <td>{product.size}</td>\r\n                    <td>{product.flavor}</td>\r\n                    <td>{product.price}</td>\r\n                    <td className=\"d-flex justify-content-end\">\r\n                        <button className=\"btn-primary\"\r\n                            onClick={() => this.load(product)}>\r\n                            <i className=\"fa fa-pencil\"></i>\r\n                        </button>\r\n                        <button className=\"btn-danger ml-2\"\r\n                            onClick={() => this.remove(product)}>\r\n                            <i className=\"fa fa-trash\"></i>\r\n                        </button>\r\n                    </td>\r\n                </tr>\r\n            )\r\n        })\r\n    }\r\n\r\n    render() {\r\n        return (\r\n            <Main {...headerProps}>\r\n                {this.renderForm()}\r\n                {this.renderTable()}\r\n            </Main>\r\n        )\r\n    }\r\n\r\n}","C:\\Users\\Ygor\\Desktop\\exercicios-js\\react\\estacaodoacai\\frontend\\src\\components\\neworder\\NewOrder.jsx",["228","229","230","231","232","233"],"import React, { Component } from 'react'\r\nimport Main from '../template/Main'\r\nimport axios from 'axios'\r\nimport ProductCrud from '../product/ProductCrud'\r\nimport './NewOrder.css'\r\nimport Loading from '../loading/Loading'\r\n\r\nconst headerProps = {\r\n    icon: 'book',\r\n    title: 'Novo pedido',\r\n    subtitle: 'Realizar um novo pedido'\r\n}\r\n\r\nconst baseUrl = 'http://localhost:3001/products'\r\nconst urlAddSimples = 'http://localhost:3001/simpleadditionals'\r\nconst urlClients = 'http://localhost:3001/clients'\r\n\r\nconst initialState = {\r\n    loading: true,\r\n    total: 0,\r\n    subtotal: 0,\r\n    product: { productName: '', description: '', price: '', size: '', id: '', flavor: '' },\r\n    client: { clientName: '', phone: '', address: '', neighborhood: '', id: '', id_neighborhood: 0, qtdpedidos: 0 },\r\n    list: [],\r\n    clientsearch: '',\r\n    listSimpleAdditionals: [{ additionalname: '', price: 2.0 }],\r\n\r\n    listaAdicionais: [] // Array que será impresso\r\n}\r\n\r\nexport default class NewOrder extends Component {\r\n\r\n    state = { ...initialState }\r\n\r\n    componentWillMount() {\r\n        axios.get(baseUrl).then(resp => {\r\n            this.setState({ list: resp.data })\r\n        })\r\n        this.getSimples()\r\n    }\r\n\r\n    getSimples() {\r\n        axios.get(urlAddSimples).then(resp => {\r\n            this.setState({ listSimpleAdditionals: resp.data })\r\n        })\r\n        return this.state.listSimpleAdditionals\r\n    }\r\n\r\n    retornoTotal() {\r\n        let total = 0;\r\n        this.state.listaAdicionais.map((adicional, index) => {\r\n            if (index == 0) {\r\n                adicional.price = 0\r\n            } else if (index == 1) {\r\n                adicional.price = 0\r\n            }\r\n            total += adicional.price\r\n            return total\r\n        })\r\n        this.setState({ total: total })\r\n    }\r\n\r\n    addNewSimpleItem2(e) {\r\n        e.preventDefault()\r\n        const list = this.state.listaAdicionais\r\n        list.push({ additionalname: '', price: 2.0 })\r\n        this.setState({ listaAdicionais: list })\r\n        this.retornoTotal()\r\n    }\r\n\r\n    removeSimpleItem() {\r\n        const list = this.state.listaAdicionais\r\n        list.pop()\r\n        this.setState({ listaAdicionais: list })\r\n        this.retornoTotal()\r\n    }\r\n\r\n    setItemSimplesValue(position, field, value) {\r\n\r\n        const updatedItemsSimples = this.state.listaAdicionais.map((adicionalItem, index) => {\r\n            if (index === position) {\r\n                return { ...adicionalItem, [field]: value };\r\n            }\r\n\r\n            return adicionalItem;\r\n\r\n        });\r\n\r\n        this.setState({ listaAdicionais: updatedItemsSimples });\r\n        console.log('Lista de adicionais: ', this.state.listaAdicionais)\r\n    }\r\n\r\n    updateFieldProduct(event) {\r\n        const product = { ... this.state.product }\r\n        product[event.target.name] = event.target.value\r\n        this.setState({ product })\r\n    }\r\n\r\n    updateFieldClient(event) {\r\n        const client = { ... this.state.client }\r\n        client[event.target.name] = event.target.value\r\n        this.setState({ client })\r\n    }\r\n\r\n    renderOptionsSimple() {\r\n        return this.state.listSimpleAdditionals.map(additional => {\r\n            return (\r\n                <option value={additional.additionalname} key={additional.id + additional.id}>{additional.additionalname}</option>\r\n            )\r\n        })\r\n    }\r\n\r\n    renderSelectSimple() {\r\n        return (\r\n            <select name=\"adicionaissimples\" className=\"form-control mt-2\" onChange={e => this.updateSelectSimple(e)}>\r\n                <option>Selecione</option>\r\n                {this.state.listSimpleAdditionals.map(additional => {\r\n                    return (\r\n                        <option>{additional.additionalname}</option>\r\n                    )\r\n                })}\r\n\r\n            </select>\r\n        )\r\n    }\r\n\r\n    renderSelectSpecial() {\r\n        return (\r\n            <select name=\"adicionaisespeciais\" className=\"form-control mt-2\">\r\n                <option>Selecione</option>\r\n                {this.renderOptionsSpecial()}\r\n            </select>\r\n        )\r\n    }\r\n\r\n    calculateTotal() {\r\n        let amount = parseFloat(this.state.total).toFixed(2)\r\n        this.state.listaAdicionais.map(adicional => {\r\n            amount = amount + adicional.price\r\n            return amount\r\n        })\r\n        return amount\r\n    }\r\n\r\n    handleChange(event) {\r\n        this.setState({ clientsearch: event.target.value });\r\n    }\r\n\r\n    loadClient(e) {\r\n        this.setState({ loading: true })\r\n        axios.get(`${urlClients}?phone=${this.state.clientsearch}`).then(resp => { // Procurando na lista\r\n            this.setState({\r\n                client: {\r\n                    clientName: resp.data[0].clientName,\r\n                    phone: resp.data[0].phone,\r\n                    address: resp.data[0].address,\r\n                    neighborhood: resp.data[0].neighborhood,\r\n                    id_neighborhood: resp.data[0].id_neighborhood,\r\n                    id: resp.data[0].id,\r\n                    qtdpedidos: resp.data[0].qtdpedidos\r\n                }\r\n            })\r\n        }).catch(err => {\r\n            alert('Cliente não cadastrado')\r\n        })\r\n\r\n        e.preventDefault()\r\n\r\n    }\r\n\r\n\r\n\r\n    renderForm() {\r\n        return (\r\n            <div>\r\n                <div className=\"form\">\r\n                    <div className=\"row d-flex justify-content-between\">\r\n                        <div className=\"ml-3\">\r\n                            <form onSubmit={e => this.loadClient(e)}>\r\n                                <input type=\"text\"\r\n                                    id=\"clientsearch\"\r\n                                    required\r\n                                    placeholder=\"Número do cliente\"\r\n                                    value={this.state.clientsearch}\r\n                                    onChange={e => this.handleChange(e)}\r\n                                />\r\n                                <button type=\"submit\">\r\n                                    <i className=\"fa fa-search\"></i>\r\n                                </button>\r\n                            </form>\r\n                        </div>\r\n\r\n                        <select className=\"form-select\">\r\n                            <option>Lista de Produtos</option>\r\n                            <option></option>\r\n                        </select>\r\n                        <select className=\"form-select mr-3\">\r\n                            <option>Lista de Bairros</option>\r\n                            <option></option>\r\n                        </select>\r\n                    </div>\r\n                    <hr />\r\n                    <div className=\"row\">\r\n                        <div className=\"col-12 col-md-3\">\r\n                            <div className=\"form-group\">\r\n                                <label>Nome do cliente</label>\r\n                                <input type=\"text\" className=\"form-control\"\r\n                                    name=\"clientName\"\r\n                                    value={this.state.client.clientName}\r\n                                    onChange={e => this.updateFieldClient(e)}\r\n                                />\r\n                            </div>\r\n                        </div>\r\n                        <div className=\"col-12 col-md-2\">\r\n                            <div className=\"form-group\">\r\n                                <label>Celular</label>\r\n                                <input type=\"text\" className=\"form-control\"\r\n                                    name=\"phone\"\r\n                                    value={this.state.client.phone}\r\n                                    onChange={e => this.updateFieldClient(e)}\r\n                                />\r\n                            </div>\r\n                        </div>\r\n                        <div className=\"col-12 col-md-4\">\r\n                            <div className=\"form-group\">\r\n                                <label>Endereço</label>\r\n                                <input type=\"text\" className=\"form-control\"\r\n                                    name=\"address\"\r\n                                    value={this.state.client.address}\r\n                                    onChange={e => this.updateFieldClient(e)}\r\n                                />\r\n                            </div>\r\n                        </div>\r\n                        <div className=\"col-12 col-md-3\">\r\n                            <div className=\"form-group\">\r\n                                <label>Bairro </label>\r\n                                <input type=\"text\" className=\"form-control\"\r\n                                    name=\"neighborhood\"\r\n                                    value={this.state.client.neighborhood}\r\n                                    onChange={e => this.updateFieldClient(e)}\r\n                                />\r\n                            </div>\r\n                        </div>\r\n                        <div className=\"col-12 col-md-3\">\r\n                            <div className=\"form-group\">\r\n                                <label>Açaí </label>\r\n                                <input type=\"text\" className=\"form-control\"\r\n                                    name=\"neighborhood\"\r\n                                    value={this.state.product.productName}\r\n                                    onChange={e => this.updateFieldClient(e)}\r\n                                />\r\n                            </div>\r\n                        </div>\r\n                        <div className=\"col-12 col-md-3\">\r\n                            <div className=\"form-group\">\r\n                                <label>Descrição </label>\r\n                                <input type=\"text\" className=\"form-control\"\r\n                                    name=\"neighborhood\"\r\n                                    value={this.state.product.neighborhood}\r\n                                    onChange={e => this.updateFieldClient(e)}\r\n                                />\r\n                            </div>\r\n                        </div>\r\n                        <div className=\"col-12 col-md-2\">\r\n                            <div className=\"form-group\">\r\n                                <label>Tamanho</label>\r\n                                <select name=\"size\" className=\"form-control\" value={this.state.product.size} onChange={e => this.updateField(e)}>\r\n                                    <option value=\"\">Selecione</option>\r\n                                    <option value=\"300\">300ml</option>\r\n                                    <option value=\"500\">500ml</option>\r\n                                    <option value=\"1000\">1000ml</option>\r\n                                </select>\r\n                            </div>\r\n                        </div>\r\n                        <div id=\"adicionais\" className=\"container col-12 col-md-4\">\r\n\r\n\r\n                            <section className=\"addsimples\">\r\n                                Adicionais Simples\r\n        